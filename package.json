{
  "name": "react-client-validation",
  "version": "1.1.0",
  "description": "client-side validation for react application",
  "main": "dist/index.min.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "rollup -c -w",
    "build": "rm -rf dist/ && rollup -c",
    "publish:message": "git add . && git commit -m 'feat: update to npm'",
    "publish:minor": "npm version minor && npm publish",
    "publish:patch": "npm version patch --no-git-tag-version && npm publish",
    "publish:major": "npm version major && npm publish",
    "git": "git config --global user.email 0529bilL@gmail.com && git config --global user.name 'bill'",
    "git:commit": "git add . && git commit -m 'fix: build' && npm version patch"
  },
  "keywords": [
    "react",
    "validation"
  ],
  "files": [
    "dist"
  ],
  "author": "water-tw",
  "license": "MIT",
  "dependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "prop-types": "^15.8.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "rollup": "^2.75.7",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0529bill/react-client-validation"
  },
  "homepage": "https://github.com/0529bill/react-client-validation"
}
